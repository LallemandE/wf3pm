{% extends 'base.html.twig' %}
{% block title %}Product Display{% endblock %}
{% block body %}
	<h1>Product</h1>
	<table>

		<tr>
			<td>Name</td>
			<td> {{ product.name }}</td>
		</tr>
		<tr>
			<td>Description </td>
			<td>{{ product.description }} </td>
		<tr>
		<tr>
			<td>Version </td>
			<td>v.{{ product.version }} </td>
		<tr>
	</table>	
{% endblock %}